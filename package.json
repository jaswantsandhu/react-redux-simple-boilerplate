{
    "name": "react-redux-simple-boilerplate",
    "version": "1.0.0",
    "description": "React redux simple boilerplate",
    "main": "index.js",
    "scripts": {
        "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
        "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
        "test:watch": "npm run test --watch",
        "prebuild": "rimraf ./dist",
        "build": "webpack -progress -p --config webpack.production.config.js"
    },
    "jscsConfig": {
        "preset": "react-redux-simple-boilerplate",
        "esnext": true,
        "requireCurlyBraces": null,
        "requireTrailingComma": null,
        "validateQuoteMarks": {
            "mark": "'",
            "escape": true,
            "ignoreJSX": true
        }
    },
    "author": "Jaswant Singh Sandhu",
    "private": true,
    "repository": "https://github.com/jaswantsandhu/react-redux-simple-boilerplate",
    "dependencies": {
        "axios": "^0.15.2",
        "babel-preset-stage-1": "^6.1.18",
        "immutability-helper": "^2.0.0",
        "lodash": "^3.10.1",
        "node-sass": "^3.11.2",
        "normalize.css": "~4.2.0",
        "qs": "^6.3.0",
        "react": "^15.3.2",
        "react-addons-css-transition-group": "^15.4.1",
        "react-addons-transition-group": "^15.3.2",
        "react-addons-update": "~15.3.0",
        "react-cookie": "^1.0.4",
        "react-css-themr": "^1.6.0",
        "react-dom": "^15.3.2",
        "react-redux": "^4.0.0",
        "react-router": "^3.0.0",
        "react-transform-catch-errors": "~1.0.0",
        "react-transform-hmr": "~1.0.1",
        "redbox-react": "~1.3.0",
        "redux": "^3.0.4",
        "redux-form": "^6.2.1",
        "redux-promise": "^0.5.3",
        "systemjs": "^0.19.40"
    },
    "devDependencies": {
        "axios-mock-adapter": "^1.7.1",
        "babel-cli": "^6.18.0",
        "babel-core": "~6.13.2",
        "babel-loader": "~6.0.1",
        "babel-plugin-react-transform": "~2.0.2",
        "babel-preset-es2015": "~6.1.4",
        "babel-preset-react": "~6.1.4",
        "babel-preset-stage-0": "~6.5.0",
        "babel-register": "^6.5.1",
        "browser-sync": "^2.17.5",
        "chai": "^3.5.0",
        "chai-jquery": "^2.0.0",
        "classnames": "~2.2.1",
        "cross-env": "~2.0.0",
        "css-loader": "~0.23.1",
        "cssobjects-loader": "^0.4.1",
        "enzyme": "^2.6.0",
        "extract-text-webpack-plugin": "~1.0.1",
        "jquery": "^2.2.4",
        "jscs": "^2.9.0",
        "jsdom": "^8.1.0",
        "mocha": "^2.4.5",
        "node-sass": "~3.8.0",
        "postcss-loader": "~0.10.1",
        "react-addons-test-utils": "^0.14.8",
        "rimraf": "^2.5.4",
        "sass-lint": "~1.7.0",
        "sass-loader": "~4.0.0",
        "sinon": "^1.17.6",
        "style-loader": "~0.13.1",
        "svg-react-loader": "^0.3.7",
        "teaspoon": "^6.1.1",
        "watch": "^0.17.1",
        "webpack": "~1.13.2",
        "webpack-dev-middleware": "~1.6.1",
        "webpack-dev-server": "^1.14.0",
        "webpack-hot-middleware": "~2.12.2",
        "webpack-livereload-plugin": "^0.9.0"
    }
}